<Window x:Class="WpfCommands.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfCommands"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button Content="Button" HorizontalAlignment="Left" Margin="119,70,0,0" VerticalAlignment="Top" Height="27" Width="78"
             Command="{Binding ButtonCommand}"  CommandParameter="heloi" />
        <Label x:Name="Label_1" Content="{Binding Propint}" HorizontalAlignment="Left" Margin="119,175,0,0" VerticalAlignment="Top" Height="28" Width="78"/>
        <Label x:Name="Label_2" Content="Label" HorizontalAlignment="Left" Margin="119,239,0,0" VerticalAlignment="Top" Height="28" Width="78"/>
        <Label x:Name="Label_3" Content="Label" HorizontalAlignment="Left" Margin="119,306,0,0" VerticalAlignment="Top" Height="28" Width="78"/>
        <Label Content="Label" HorizontalAlignment="Left" Margin="551,267,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
